#include<stdio.h>

double findMedianSortedArrays(int* nums1, int nums1Size, int*nums2, int nums2size)
{
int i = 0;
int j = 0;
int cnt;
int x = -1, y = -1;

for (cnt = 0; cnt <= n; cnt++)
{
if (i == n)
{
x = y;
y = nums1;
break;
}
else if (j == n)
{
x = y;
y = nums1[0];
break;
}

if (nums1[i] < nums2[j])
{
x = y;
y = nums1[i];
i++;
}
else
{
x = y;
y = nums2[j];
j++;}
}
}
